<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
    "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
  <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="false" />

    <package name="index" namespace="/"   extends="struts-default"  strict-method-invocation="false" >
  	<action name="index_*" class="indexbean" method="{1}">
  	</action>
  	<action name="notice_*" class="noticebean" method="{1}">
  	</action>
  	 <action name="article_*" class="articlebean" method="{1}">
    </action>
	 <action name="product_*" class="productbean" method="{1}">
  	</action>
  	<action name="message_*" class="messagebean" method="{1}">
  	</action>
</package>
</struts>